<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>
<%= form_for @user , url:  users_path do|f| %>
	<p>
		<%= f.label :Name %>
		<%= f.text_field :name %> <br>
	</p>
	<p>
		<%= f.label :Password %>
		<%= f.password_field :password %> <br>
	</p>
	<p>
		<%= f.submit "Create User" %>
	</p>
<% end %>